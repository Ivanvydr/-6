def ls_sorted(t):
  return t == sorted(t)
  
  
6)

def is_anagram(word1, word2):
  return sorted(word1) == sorted(word2)
  
  
7)

def has_duplicates(s):
  x = 'False'
  t = sorted(s)
  for i in range(len(t)):
    if t[i] == t[t-i]:
      x = 'True'
      break
  return x
